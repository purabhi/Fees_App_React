{

  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.js"
    }
  ],
    "builds": [
      {
        "src": "client/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
    
  }
  